{
  "canisters": {
    "htv9_backend": {
      "build": "python -m kybra htv9_backend src/htv9_backend/src/main.py src/htv9_backend/htv9_backend.did",
      "candid": "src/htv9_backend/htv9_backend.did",
      "gzip": true,
      "post_install": ".kybra/htv9_backend/post_install.sh",
      "tech_stack": {
        "cdk": {
          "kybra": {}
        },
        "language": {
          "python": {}
        }
      },
      "type": "custom",
      "wasm": ".kybra/htv9_backend/htv9_backend.wasm"
    },
    "htv9_frontend": {
      "dependencies": [
        "htv9_backend"
      ],
      "source": [
        "src/htv9_frontend/dist"
      ],
      "type": "assets",
      "workspace": "htv9_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}